# Copy this file to .env.local
# The app works with placeholder values using fallback data

# Sanity CMS Configuration
# Use 'placeholder' to run with sample data (no CMS needed)
# Or replace with your actual Project ID from https://www.sanity.io/manage
NEXT_PUBLIC_SANITY_PROJECT_ID=placeholder
NEXT_PUBLIC_SANITY_DATASET=production

# Optional: Sanity API Token (for preview mode or mutations)
# SANITY_API_TOKEN=your_token_here

